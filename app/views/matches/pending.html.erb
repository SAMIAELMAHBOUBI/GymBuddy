<h1>Pending Matches</h1>
<%= @pending_matches.each do |match|%>
  <%=match.profile_id %>
<%end%>
<% @profiles.each do |profile| %>
  <div>
    <p><%= profile.first_name %> <%= profile.last_name %></p>
    <%= button_to "Accept", accept_profile_match_path(@profile), method: :patch, class: "btn btn-success" %>
    <%= button_to "Reject", reject_profile_match_path(@profile), method: :patch, class: "btn btn-danger" %>
  </div>
<% end %>
