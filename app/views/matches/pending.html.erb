<h1>Pending Matches</h1>

<% @profiles.each do |profile| %>
  <div>
    <p><%= profile.first_name %> <%= profile.last_name %></p>
    <%= button_to "Accept", accept_match_path(profile_id: profile.id), method: :patch, class: "btn btn-success" %>
    <%= button_to "Reject", reject_match_path(profile_id: profile.id), method: :patch, class: "btn btn-danger" %>
  </div>
<% end %>
